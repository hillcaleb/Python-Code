    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "100\n"
     ]
    }
   ],
   "source": [
    "#uses a for loop to add 1 to x until it reaches 100 \n",
    "for x in range(100): #defines for loop so that it iterates 100 times\n",
    "    x +=1  #adds one to x\n",
    "print(x) #prints x"
   ]
  },
  {
   "cell_type": "code",
